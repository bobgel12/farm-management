    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.5",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
